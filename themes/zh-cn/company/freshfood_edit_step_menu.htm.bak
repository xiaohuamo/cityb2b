<div class="input-box input-submit clearfix">
	<{if $step&&$step!='1'}>
	<button type="button" onClick="window.location.href='<{$prevUrl}>';" style="float:left;">上一步</button>
	<{/if}>
	
	
	<button type="submit" style="float:right;margin-left :40px;"> 
		<{if $step&&$step=='6'}>
		
		
			完成
		
		
		<{elseif  $step&&$step=='1'}>
		下一步
		<{else}>
		下一步
		<{/if}></button>
		
</div>



