			<div class="order-panel flexbox2 rsection1 rsection1Box F14 J_product_cate">
			<!-- 订单统配号 列表 -->
				<{for $var=1 to 50}>
			<!--	<span class='btnbox printed'><{$var}></span> -->
				<{/for}>
				</div>
				</div>
		
	</div>


	<div class='panel-right'>
		<!-- right -->
		
		<div>
			<!-- 打印份数 -->
			<div class='copy-adjust-panel' style="margin: 1rem 1rem;">
				<!-- 快速按钮 1-6 -->
