									success:function(data){
									
										var html = '';
										var trukNo = 0;
										var isselect=0;
										var truckanddriver ='';
										
										var selectTruck ='<{$logistic_truck_No}>';
																				
										if(selectTruck=='all' || selectTruck.length ==0 || !selectTruck) {
										  html ='<option value="all" selected ><{$lang->logistic_choosetruck}></option>';
										
										}else{
										  html ='<option value="all"><{$lang->logistic_choosetruck}></option>';
										}
										
										for (var i = 0; i< data.length ; i++) {
										
											
										truckNo = data[i]['logistic_truck_No'];
										
										if(truckNo ==0) {
										    if (trukNo ===selectTruck){
											   html += "<option value="+data[i]['logistic_truck_No']+" selected  ><{$lang->no_truck_yet}></option>";
											  
											}else{
											    html += "<option value="+data[i]['logistic_truck_No']+" ><{$lang->no_truck_yet}></option>";
											}
										}else{
										
										    if(data[i]['driverName']){
											
											   truckanddriver = data[i]['driverName'] +'-'+data[i]['truck_name']+"-"+data[i]['plate_number'] +"("+data[i]['count']+")";
											}else{
												truckanddriver = data[i]['truck_name']+"-"+data[i]['plate_number']+"("+data[i]['count']+")";
											}
										
										   if (trukNo ===selectTruck){
											 
											   html += "<option value="+data[i]['logistic_truck_No']+" selected>"+truckanddriver+"</option>";
											}else{
												html += "<option value="+data[i]['logistic_truck_No']+" >"+truckanddriver+"</option>";
											}
										
										
										}
							


									}
										$('.chooselogistic_truck_No').html(html);
									},
									error:function(data){
										console.log(data);
									}
								})
							})
							
							$('.chooselogistic_truck_No').on('change',function(){
							   $('#printer').css({ "display": "none" });
								
							})

