            return `<span style="height:16px;background-color: grey;color: white" id="menu-category-${menuId}-${categoryId}">${categories[categoryId + '']} <a style="color:red" onclick="deleteMenuCategory(${menuId}, ${categoryId})">X&nbsp;</a></span>`;
        }
		</script>
