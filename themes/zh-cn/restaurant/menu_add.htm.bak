            data: {"id": couponId},
            beforeSend: function (xhr) {
                loading = true;

                context.html("<i style='color:#2bb8aa' class='fa fa-spinner fa-pulse'></i>");
            },
            success: function (result) {
                result = jQuery.parseJSON(result);

